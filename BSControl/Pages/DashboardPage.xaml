<Page x:Class="BSControl.Pages.DashboardPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BSControl.Pages"
      xmlns:expControl="clr-namespace:BSControl;assembly=BSControl"
      DataContext="{Binding RelativeSource={RelativeSource Self}}"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Background="AntiqueWhite"
      Title="DashboardPage">
    <Grid>
        <!--<StackPanel Name="UserControls" Orientation="Vertical">
            
        </StackPanel>-->
        <StackPanel>
            <expControl:BSControl ProtocolNumber="{Binding Path=Number, Mode=TwoWay}" />
        </StackPanel>
    </Grid>
</Page>
